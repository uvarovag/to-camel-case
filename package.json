{
    "name": "@uvarovag/to-camel-case",
    "version": "1.0.14",
    "description": "Hard conversion of a string to camel case",
    "main": "dist/index.js",
    "module": "dist.es2015/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "dist.es2015"
    ],
    "scripts": {
        "build:cjs": "tsc --project tsconfig.cjs.json",
        "build:esm": "tsc --project tsconfig.esm.json",
        "build": "npm run build:cjs && npm run build:esm",
        "lint:fix": "prettier --write '**/*.{ts,tsx,js,json,css,html,md}' && eslint '**/*.{ts,tsx}' --fix && npm pkg fix",
        "test": "npx jest",
        "publish": "npm run lint:fix && npm run test && npm run build && npm publish --access public"
    },
    "keywords": [
        "camelcase",
        "string-formatting",
        "string-utils",
        "convert-case",
        "latin",
        "cyrillic",
        "javascript",
        "typescript",
        "npm-package",
        "lightweight",
        "camel-case",
        "string-manipulation"
    ],
    "author": "@uvarovag",
    "license": "MIT",
    "homepage": "https://github.com/uvarovag/to-camel-case/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uvarovag/to-camel-case.git"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.1",
        "@typescript-eslint/eslint-plugin": "^8.17.0",
        "@typescript-eslint/parser": "^8.17.0",
        "@uvarovag/prettier-config-ts-react": "^1.0.6",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.2"
    }
}
